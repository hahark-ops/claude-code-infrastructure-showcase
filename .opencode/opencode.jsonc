{
  "$schema": "https://opencode.ai/config.json",
  "plugin": [
    "oh-my-opencode@3.5.2",
    "./.opencode/plugins/hook-compat.ts"
  ],
  "provider": {
    "openai": {
      "name": "OpenAI",
      "models": {
        "gpt-5.3-codex": {
          "name": "GPT-5.3 Codex",
